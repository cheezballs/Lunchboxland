config_version=5

[application]

config/name="Lunchbox Land"
config/description="An online virtual hangout built in Godot 4. This is a work in progress sandbox open to the community to help us build better multiplayer games. "
config/features=PackedStringArray("4.4", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

ApplicationManager="*res://core/ApplicationManager.cs"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Lunchboxland"
